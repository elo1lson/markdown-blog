<%- include('includes/header') %>
<%- include('includes/nav') %>
<div class="my-5">
</div>
<% titles.forEach(t => { %>

    <div class="card mb-3 mx-auto my-2" style="max-width: 940px; width:90%;">
        <div class="row g-0">
          <div class="col-md-3">
            <img class="img-thumbnail" src="<%- t[2] %> " class="img-fluid rounded-start" alt="img-thumbnail" style="max-width: 95%;">
        </div>
          <div class="col-md-8">
            <div class="card-body">
              <h5 class="card-title"><%- t[0] %></h5>
              <p class="card-text"><%- t[1] %> </p>
              <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
              <a href="?name=<%- encodeURI(t[0]) %>" class="btn btn-primary">Ler</a>
           
            </div>
          </div>
        </div>
      </div>
<% }) %>
 
<%- include('includes/footer') %>